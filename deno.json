{
  "nodeModulesDir": "auto",
  "imports": {
    "@punkpeye/fastmcp": "jsr:@punkpeye/fastmcp@^3.26.7",
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@std/path": "jsr:@std/path@^1.1.2",
    "@zod/zod": "jsr:@zod/zod@^4.1.12"
  },
  "tasks": {
    "dev": "PREFRONTAL_TRANSPORT=stdio deno run -A src/server.ts",
    "serve": "PREFRONTAL_TRANSPORT=http deno run -A scripts/ensure_ollama_model.ts && deno run -A scripts/ensure_qdrant.ts && deno run -A src/server.ts",
    "start": "deno run -A src/server.ts",
    "cli": "deno run -A src/cli/main.ts",
    "tui": "deno run --allow-run --allow-env src/tui/main.ts",
    "ensure:ollama": "deno run -A scripts/ensure_ollama_model.ts",
    "ensure:qdrant": "deno run -A scripts/ensure_qdrant.ts",
    "smoke": "deno run -A scripts/smoke.ts",
    "fmt": "deno fmt",
    "check": "deno check src/server.ts",
    "test": "deno test tests/schemas_test.ts"
  }
}
